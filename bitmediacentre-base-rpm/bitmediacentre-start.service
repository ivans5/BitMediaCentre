#Note: according https://www.freedesktop.org/wiki/Software/PulseAudio/FAQ/
#XDG_RUNTIME_DIR is supposed to be on ramdisk!

[Unit]

[Service]
Type=oneshot
ExecStartPre=chattr -i /
ExecStart=/bin/sh -c "[ -e /env.sh ] || ln -s /usr/start/env.sh /env.sh; [ -e /start ] || { mkdir -p /var/start/; ln -s /usr/start/youtube /var/start/youtube; ln -s /var/start /start; ln -s /var/home/pcuser /homepcuser; }; mkdir -p /tmp/xdg_runtime_dir; chown pcuser:pcuser /tmp/xdg_runtime_dir; chmod 700 /tmp/xdg_runtime_dir; chcon -u system_u -t user_tmp_t /tmp/xdg_runtime_dir"
ExecStopPost=chattr +i /

[Install]
WantedBy=basic.target
